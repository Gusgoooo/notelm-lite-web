{
  "name": "notelm-lite-web",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter @notelm/web dev\" \"pnpm --filter @notelm/api dev\"",
    "build": "pnpm -r run build",
    "build:prod": "pnpm --filter @notelm/db db:migrate:deploy && pnpm build",
    "db:seed": "pnpm --filter @notelm/db db:seed",
    "postinstall": "pnpm --filter @notelm/db db:generate"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  },
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=20"
  }
}
